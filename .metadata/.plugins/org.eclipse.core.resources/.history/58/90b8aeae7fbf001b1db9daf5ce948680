				//montab[i]=montab[j];
				//break;
			//}
			
	//	}
		
		for(int j =i+1;j<montab.length;j++)
		if (montab[i]<montab [j])
		{
		montab[i]=montab[j];
			
			
		}
	}	
	
	for (int i = 0; i < montab.length; i++) 
	{
	
		
	System.out.println(montab[i]+" ");
		
		
	}	
	}

}
